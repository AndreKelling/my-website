<link rel="canonical" href="{{ site.siteURL }}{{ path }}"/>

<meta charset="utf-8"/>
<meta http-equiv="x-ua-compatible" content="ie=edge"/>
<meta name="HandheldFriendly" content="True"/>
<meta name="MobileOptimized" content="320"/>
<meta name="viewport" content="width=device-width, initial-scale=1"/>
<meta http-equiv="cleartype" content="on"/>

<title>{{ seo.title }}</title>
<meta name="description" content="{{ seo.description }}"/>
<meta name="author" content="AndrÃ© Kelling"/>
<meta name="generator" content="metalsmith"/>

<link rel="apple-touch-icon" sizes="180x180" href="{{ basePath }}/assets/icons/apple-touch-icon.png?v={{ faviconHash }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ basePath }}/assets/icons/favicon-32x32.png?v={{ faviconHash }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ basePath }}/assets/icons/favicon-16x16.png?v={{ faviconHash }}">
<link rel="manifest" href="{{ basePath }}/site.webmanifest?v={{ faviconHash }}">
<link rel="mask-icon" href="{{ basePath }}/assets/icons/safari-pinned-tab.svg?v={{ faviconHash }}" color="#99af78">
<meta name="msapplication-TileColor" content="#8A9478">
<meta name="theme-color" content="#8A9478">

<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:site" content="{{ site.description }}"/>
<meta name="twitter:title" content="{{ site.title }}"/>
<meta name="twitter:description" content="{{ site.description }}"/>
<meta name="twitter:creator" content="{{ site.author }}"/>
<meta name="twitter:image" content="{{ basePath }}{{ seo.socialImage }}"/>
<meta property="og:locale" content="en_US"/>
<meta property="og:type" content="article"/>
<meta property="og:title" content="{{ site.title }}"/>
<meta property="og:description" content="{{ site.description }}"/>
<meta property="og:url" content="{{ site.siteURL }}"/>
<meta property="og:site_name" content="{{ site.title }}"/>
<meta property="og:image" content="{{ basePath }}{{ seo.socialImage }}"/>
<meta property="og:updated_time" content=""/>

<link rel="preload" href="{{ basePath }}/style.css?v={{ styleHash }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{ basePath }}/style.css?v={{ styleHash }}"></noscript>
<script src="{{ basePath }}/s.js?v={{ scriptHash }}" async></script>

